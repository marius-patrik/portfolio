{
	"name": "liqid-components",
	"version": "0.0.1",
	"description": "React component library inspired by Apple's Liquid Glass design",
	"author": "Patrik Marius",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/marius-patrik/liqid"
	},
	"private": false,
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./styles.css": "./dist/esm/index.css"
	},
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/esm/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "rslib build",
		"dev": "rslib build --watch",
		"format": "biome format --write",
		"lint": "biome check --write",
		"lint:check": "biome check"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@rsbuild/plugin-react": "^1.1.1",
		"@rslib/core": "^0.6.0",
		"@tailwindcss/postcss": "^4.0.0",
		"@types/node": "^25.0.3",
		"@types/react": "^19.0.2",
		"@types/react-dom": "^19.0.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.7.2"
	},
	"peerDependencies": {
		"react": ">=17.0.0",
		"react-dom": ">=17.0.0"
	},
	"dependencies": {
		"swr": "^2.3.8",
		"wouter": "^3.9.0"
	}
}
